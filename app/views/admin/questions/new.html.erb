<%= question_header(@test.title, :new) %>
<%= form_with model: [:admin, @test, @question], local: true do |form| %>
  <%= render 'shared/errors', resource: @question %>
  <div class="form-group"><%= render 'form_fields', form: form %></div>

  <p><%= form.submit %></p>
<% end %>
<%= link_to 'Back to Test', admin_test_path(@test) %>
