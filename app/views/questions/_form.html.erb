<%= form_with model: @question, url: url, local: true do |form| %>
  <%= render 'shared/errors', resource: @question %>
  <p>
    <%= form.label :question %><br>
    <%= form.text_field :body %>
  </p><br>

  <%= form.hidden_field :test_id, value: params[:test_id] %>
  <p><%= form.submit %></p>
<% end %>